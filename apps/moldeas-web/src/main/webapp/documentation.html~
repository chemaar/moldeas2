      nuts:shapeKML &lt;http://geoservice.psi.enakting.org/nuts/polygon?nuts_id=ES120&format=kml&gt; ;
      nuts:validity 
	&lt;http://purl.org/weso/moldeas/nuts/resource/v3&gt; , 
