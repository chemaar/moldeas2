      &lt;http://www.w3.org/2001/vcard-rdf/3.0#TEL&gt;
              &lt;http://purl.org/weso/moldeas/organization/resource/ca170#tel&gt; .
</pre>

	</li>
		<li>A NUTS region (enriched)...
		<pre class="prettyprint lang-sql">
&lt;http://purl.org/weso/moldeas/nuts/resource/ES120&gt;
      a       nuts:NUTSRegion ;
      rdfs:label "Asturias" ;
      spatialrelations:containedBy
              &lt;http://purl.org/weso/moldeas/nuts/resource/ES12&gt; ;
