    <from>^/area[(\s)|(\/)]*$</from>
    <to type="redirect">/moldeas/area/ds</to>
</rule>

<rule>
    <from>^/area/region[(\s)|(\/)]*$</from>
    <to type="redirect">/moldeas/area/region/ds</to>
</rule>

<rule>
    <from>^/area/country[(\s)|(\/)]*$</from>
    <to type="redirect">/moldeas/area/country/ds</to>
</rule>
        
<rule>
    <from>^/organization[(\s)|(\/)]*$</from>
    <to type="redirect">/moldeas/organization/ds</to>
</rule>

<rule>
    <from>^/people[(\s)|(\/)]*$</from>
    <to type="redirect">/moldeas/people/ds</to>
</rule>


</urlrewrite>
